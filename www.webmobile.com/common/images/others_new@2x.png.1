                       rate_url:rate_url,
                       ratingPoints:currentrate,
                       action: "userRating"
                   },
                   success: function (response) {
                       var obj = JSON.parse(response)
                       var totalClient = obj.totalClient;
                       var totalRating = obj.totalRating;
                       $("#ratingValue").html(totalRating);
                       $("#totalClient").html(totalClient);
                       $('.rateit-wrap').find('.rate-btn.rateYo2').addClass('edited');
                       $('.rateit-wrap').find('.rate-btn.rateYo2').text('Rated');
                       $("#rateYo1").rateYo("option", "readOnly", true);
                   }
               });
           },
           rating: ratingValue,
           halfStar: true,
           starWidth: "13px",
           ratedFill: "#fdc532",
           normalFill: "#ffffff",
           spacing: "2px",
           readOnly: true
       });
   
       $("#rateYo1 svg").click(function (e) {
   
          
           if ($('#rateYo1').hasClass('rateYo2') && !$('#totalClient').hasClass('mark')) {
              
               /* get rating */            
               $("#totalClient").addClass("mark");
               $(this).addClass('sucess');
               
               setTimeout(function () {
                   $('.rate-submit').fadeOut(500);
               }, 1000);
           } else if ($('#rateYo1').hasClass('rateYo2') && $('#totalClient').hasClass('mark')) {
               $('.rate-btn.rateYo2').text('Already Rated');
               $('.rate-btn.rateYo2').addClass('already_rated');
   
           } else {
               e.preventDefault();
               return false;
           }
   
       });
       $('.rate-btn').on('click', function (e) {
           if ($('.edited').length > 0) {
               e.preventDefault();
               return false;                         
           }
           else {
               $("#rateYo1").rateYo("option", "readOnly", false);
               $(".rate-submit").addClass("full-show");
               $("#rateYo1").addClass("rateYo2");
               $(this).addClass("rateYo2");
               $(this).removeClass("enable");
               $(this).text("Click to rate");
           }
       });
</script>
