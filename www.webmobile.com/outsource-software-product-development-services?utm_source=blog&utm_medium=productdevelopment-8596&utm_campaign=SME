   <script>
       function showShareURL() {
            $("#share_url").show();
            $("#label_share_url").hide();
        }
   </script>
  <script type="text/javascript">
$(document).ready(function() {
    $("#contact_pdf_request").submit(function() {  
	    var proceed = true;
        //simple validation at client's end
        //loop through each field and we simply change border color to red for invalid fields		
		$("#contact_form input[required=true]").each(function(){
			$(this).css('border-color',''); 
			if(!$.trim($(this).val())){ //if this field is empty 
				$(this).css({'border':'1px solid #FF0000', 'box-shadow':'0 0 5px red'}); //change border color to red   
				proceed = false; //set do not proceed flag
			}
			//check invalid email
			var email_reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/; 
			if($(this).attr("type")=="email" && !email_reg.test($.trim($(this).val()))){
				$(this).css({'border':'1px solid #FF0000', 'box-shadow':'0 0 5px red'}); //change border color to red   
				proceed = false; //set do not proceed flag				
			}	
		});
       
        if(proceed) //everything looks good! proceed...
        {

           //data to be sent to server         
            var m_data = new FormData();    
            m_data.append( 'user_name', $('#contact_form input[name=name]').val());
            m_data.append( 'user_email', $('#contact_form input[name=email]').val());
            m_data.append( 'company_name', $('#contact_form input[name=company_name]').val());
            
            if($('#subscribe').is(':checked')) m_data.append( 'subscribe', 'Yes');
            else m_data.append( 'subscribe', 'No');
			 
            //instead of $.post() we are using $.ajax()
            //that's because $.ajax() has more options and flexibly.
  			$.ajax({
              url: 'common/inc/contact_me.php',
              data: m_data,
              processData: false,
              contentType: false,
              type: 'POST',
              dataType:'json',
              success: function(response){
				$('#contact_form input[name=name]').val('').css({'border':'1px solid #cfcfcf', 'box-shadow':'none'});
				$('#contact_form input[name=email]').val('').css({'border':'1px solid #cfcfcf', 'box-shadow':'none'});
				$('#contact_form input[name=company_name]').val('').css({'border':'1px solid #cfcfcf', 'box-shadow':'none'});
				$('#subscribe').prop('checked', false);
				 
                //load json data from server and output message     
 				if(response.type == 'error'){ //load json data from server and output message     
					output = '<div class="error">'+response.text+'</div>';
				}else{
				    $('.contact-description').addClass('hide-me').hide();
				    $('#contact_body').addClass('hide-me').hide();
				    output = '<div class="success">'+response.text+'</div>';
				}
				$("#contact_form #contact_results").hide().html(output).slideDown();
              }
            });
			

        }
    });
    
    //reset previously set border colors and hide all message on .keyup()
    $("#contact_form  input[required=true]").keyup(function() { 
        $(this).removeAttr("style");
        //$(".success, .error").slideUp();
    });
});
</script>

