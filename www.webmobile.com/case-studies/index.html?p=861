<script type="text/javascript">   
(function(jQuery){ 

jQuery('.case_study_slides').bxSlider({
  minSlides: 2,
  maxSlides: 2,
 slideWidth: 530,
  slideMargin: 20,
  auto: true,
  wrapperClass: 'slider',
  useCSS: true,
});
jQuery('.related-caseStudies-box-result').bxSlider({
 mode: 'horizontal',
infiniteLoop: true,
auto: true,
autoStart: true,
/*autoDirection: 'next',*/
autoHover: true,
useCSS: true,
autoControls: false,
pager: true,
pagerType: 'full',
//controls: true,
//captions: true,
speed: 500,
});
    var jQueryratevalue = jQuery("#rateYo1").rateYo({ 
        onSet: function (rating, rateYoInstance) {
            rate_url = jQuery("#rating_page_url").val();
            currentrate = jQuery("#currentrate").val();
            jQuery.ajax({
                type: "POST",
                url: SITE_ROOT_URL+"ajax_act.php?t=" + Math.floor((Math.random() * 1000000) + 1),
                dataType: "html",
                data: {
                    rating: rating,
                    rate_url:rate_url,
                    ratingPoints:currentrate,
                    action: "userRating"
                },
                success: function (response) {
                    var obj = JSON.parse(response)
                    var totalClient = obj.totalClient;
                    var totalRating = obj.totalRating;
                    jQuery("#ratingValue").html(totalRating);
                    jQuery("#totalClient").html(totalClient);
                    jQuery('.rateit-wrap').find('.rate-btn.rateYo2').addClass('edited');
                    jQuery('.rateit-wrap').find('.rate-btn.rateYo2').text('Rated');
                    jQuery("#rateYo1").rateYo("option", "readOnly", true);
                }
            });
        },
        rating: ratingValue,
        halfStar: true,
        starWidth: "13px",
        ratedFill: "#fdc532",
        normalFill: "#ffffff",
        spacing: "2px",
        readOnly: true
    });

    jQuery("#rateYo1 svg").click(function (e) {

       
        if (jQuery('#rateYo1').hasClass('rateYo2') && !jQuery('#totalClient').hasClass('mark')) {
           
            /* get rating */            
            jQuery("#totalClient").addClass("mark");
            jQuery(this).addClass('sucess');
            
            setTimeout(function () {
                jQuery('.rate-submit').fadeOut(500);
            }, 1000);
        } else if (jQuery('#rateYo1').hasClass('rateYo2') && jQuery('#totalClient').hasClass('mark')) {
            jQuery('.rate-btn.rateYo2').text('Already Rated');
            jQuery('.rate-btn.rateYo2').addClass('already_rated');

        } else {
            e.preventDefault();
            return false;
        }

    });
    jQuery('.rate-btn').on('click', function (e) {
        if (jQuery('.edited').length > 0) {
            e.preventDefault();
            return false;                         
        }
        else {
            jQuery("#rateYo1").rateYo("option", "readOnly", false);
            jQuery(".rate-submit").addClass("full-show");
            jQuery("#rateYo1").addClass("rateYo2");
            jQuery(this).addClass("rateYo2");
            jQuery(this).removeClass("enable");
            jQuery(this).text("Click to rate");
        }
    });
    })(jQuery);
</script>

