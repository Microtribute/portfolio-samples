<!-- Start ladda JS -->
<script>
    $("li.menu_arrow").hover(function() {
        $(".sub_menu").hide();
        $(this).find(".sub_menu").show();
    }, function() {
        $(".sub_menu").hide();
    });


    Ladda.bind('.ladda-button');
    $('.ladda-button').click(function(e) {

        e.preventDefault();
        var url = $(this).attr('href');
        console.log($(this).attr('href'));
        var startTime;
        $.ajax({
            url: url,
            beforeSend: function(xhr) {
                startTime = +new Date();
            },
            complete: function(xhr, state) {
                var latency = (+new Date()) - startTime;
                window.console.log(latency);
                setTimeout(function() {
                    location.href = url;
                }, latency);
            }
        });
    });
    $('#submitFrm').click(function() {
        if ($('#submitFrm').hasClass('ladda-button1')) {
            return true;
        } else {
            $('#submitFrm').addClass('ladda-button');
            $('#submitFrm').addClass('ladda-button1');
            $('#user-name,#user-email,#user-phone,#user-country-div').removeClass('error');
            $('#user-name-err,#user-email-err,#user-phone-err,#user-country-err').html('');
            var emailvalid = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,17}|[0-9]{1,3})(\]?)$/;
            //var phonevalid = /^[0123456789().+-]+$/;
            var phonevalid = /^[a-zA-Z0-9.!@?#"$%&:';()*\+,\/;\-=[\\\]\^_{|}<>~` ]+$/;
            var letters = /^[A-Za-z ]{2,}/;
            var username = $.trim($('#user-name').val());
            var useremail = $.trim($('#user-email').val());
            var userphone = $.trim($('#user-phone').val());
            //var usercountry = $.trim($('#user-country').val());

            if (username == "Name")
            {
                $('#user-name').addClass('error');
                $('#user-name-err').html('Please enter valid name');
                $('#user-name').focus();
                $('#submitFrm').removeClass('ladda-button');
                return false;
            }


            if (!letters.test(username))
            {
                $('#user-name').addClass('error');
                $('#user-name-err').html('Name should be alphabets only');
                $('#submitFrm').removeClass('ladda-button');
                $('#user-name').focus();
                return false;
            }


            if (useremail == "Email")
            {
                $('#user-email').addClass('error');
                $('#user-email-err').html('Please enter valid email id');
                $('#submitFrm').removeClass('ladda-button');
                $('#user-email').focus();
                return false;
            }
            if (!emailvalid.test(useremail))
            {
                $('#user-email').addClass('error');
                $('#user-email-err').html('Please enter valid email id');
                $('#submitFrm').removeClass('ladda-button');
                $('#user-email').focus();
                return false;
            }

            /*if (usercountry == "")
            {
                $('#user-country-div').addClass('error');
                $('#user-country-err').html('Please select country');
                $('#submitFrm').removeClass('ladda-button');
                return false;
            }*/

            if (userphone == "Phone")
            {
                $('#user-phone').addClass('error');
                $('#user-phone-err').html('Please enter valid contact no.');
                $('#submitFrm').removeClass('ladda-button');
                $('#user-phone').focus();
                return false;
            }
            if (!phonevalid.test(userphone))
            {
                $('#user-phone').addClass('error');
                $('#user-phone-err').html('Please enter valid phone number');
                $('#submitFrm').removeClass('ladda-button');
                $('#user-phone').focus();
                return false;
            }

            var l = Ladda.create(document.querySelector('.ladda-button1'));
            l.start();
            $('#contact-frm').submit();
        }
    });
</script>

<!-- Tab menu scripts end -->
